{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Expressions"
        ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning",
                "Hangfire.Server.ServerJobCancellationWatcher": "Warning",
                "Hangfire.PostgreSql.CountersAggregator": "Warning",
                "Hangfire.PostgreSql.ExpirationManager": "Warning",
                "Hangfire.Processing.BackgroundExecution": "Warning",
                "Hangfire.Server.ServerHeartbeatProcess": "Warning"
            }
        },
        "Filter": [
            {
                "Name": "ByExcluding",
                "Args": {
                    "expression": "@l='Error' and StartsWith(@x,'ApiCourseShot.Infrastructure.HttpStatusException')"
                }
            },
            {
                "Name": "ByExcluding",
                "Args": {
                    "expression": "@l='Error' and SourceContext='Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware' and @mt='An unhandled exception has occurred while executing the request.'"
                }
            }
        ],
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "formatter": {
                        "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
                        "template": "[{@t:yyyy-MM-dd HH:mm:ss.fff zzz}] [{@l}]\n{#if SourceContext  is not null}SourceContext: {SourceContext}\n{#end}{#if EventId  is not null}EventId: {EventId}\n{#end}{#if RequestPath  is not null}RequestPath: {RequestPath}\n{#end}{#if ActionName  is not null}ActionName: {ActionName}\n{#end}{@m}\n{@x}\n"
                    }
                }
            }
        ]
    },
    "Database": {
        "ConnectionString": "Host=localhost;Port=5432;Database=database;Username=root;Password=qwerty;Include Error Detail=true;GssEncryptionMode=Disable"
    },
    "Application": {
        "FileBasePath": "app/files/media"
    },
    "Redis": {
        "Configuration": "redis_stack:6379",
        "InstanceName": "app:"
    },
    "RabbitMq": {
        "Host": "localhost",
        "VHost": "vhost",
        "Port": "5672",
        "UserName": "UserName",
        "Password": "UserName"
    }
}